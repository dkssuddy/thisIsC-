#include <iostream>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	long long int ans[91][2]; // [0]은 0으로 끝나는거 [1]은 1로 끝나는거
	ans[1][0]=0;
	ans[1][1]=1;
	for(int i=2; i<91;i++)
	{
		ans[i][0] = ans[i-1][0] +ans[i-1][1];
		ans[i][1] = ans[i-1][0];
  }
	int N;
	cin >> N;
	cout << ans[N][0]+ans[N][1];

	return 0;
}
